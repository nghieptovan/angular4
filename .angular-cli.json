{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "lottevn"
  },
  "apps": [
    {
      "root": "frontend",
      "outDir": "dist/build",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "app.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/nouislider/distribute/nouislider.min.css",
        "../node_modules/owl.carousel/dist/assets/owl.carousel.css",
        "../node_modules/owl.carousel/dist/assets/owl.theme.default.css",
        "../node_modules/ngx-bar-rating/themes/br-default-theme.css",
        "../node_modules/ngx-toastr/toastr.css",
        "../node_modules/slick-carousel/slick/slick.css",
        "../node_modules/ng-masonry-grid/ng-masonry-grid.css"
      ],
      "scripts": [
        "assets/vendors/js/core/jquery-3.2.1.min.js",
        "assets/vendors/js/core/popper.min.js",
        "assets/vendors/js/core/bootstrap.min.js",
        "assets/vendors/js/perfect-scrollbar.jquery.min.js",
        "assets/vendors/js/prism.min.js",
        "assets/vendors/js/jquery.matchHeight-min.js",
        "assets/vendors/js/screenfull.min.js",
        "assets/vendors/js/pace/pace.min.js",
        "assets/vendors/js/chartist.min.js",    
        "assets/vendors/js/datatable/datatables.min.js",
        "assets/vendors/js/datatable/dataTables.buttons.min.js",
        "assets/vendors/js/datatable/buttons.flash.min.js",
        "assets/vendors/js/datatable/jszip.min.js",
        "assets/vendors/js/datatable/pdfmake.min.js",
        "assets/vendors/js/datatable/vfs_fonts.js",
        "assets/vendors/js/datatable/buttons.html5.min.js",
        "assets/vendors/js/datatable/buttons.print.min.js",
        "assets/js/data-tables/datatable-advanced.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "develop": "environments/environment.dev.ts",
        "stag": "environments/environment.stag.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "frontend/tsconfig.app.json"
    },
    {
      "project": "frontend/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "less",
    "component": {}
  }
}
